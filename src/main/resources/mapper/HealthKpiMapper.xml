<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tencent.wxcloudrun.dao.HealthKpiMapper">

    <resultMap id="healthKpiMapper" type="com.tencent.wxcloudrun.model.HealthKpiDto">
        <id property="monthAge" column="MONTH_AGE"/>
        <result property="monthAgeDesc" column="MONTH_AGE_DESC"/>
        <result property="kpiType" column="KPI_TYPE" />
        <result property="kpiCode" column="KPI_CODE" />
        <result property="kpiDesc" column="KPI_DESC" />
        <result property="score" column="SCORE" />
        <result property="createdTime" column="CREATED_DATE"  />
    </resultMap>

    <select id="getAll" resultMap="healthKpiMapper" >
        SELECT *
        FROM dw_child_health_kpi_d
    </select>

</mapper>
